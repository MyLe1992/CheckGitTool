<d:ctr type="AUTOSAR" factory="autosar" 
    xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd" 
    xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd" 
    xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
    <d:lst type="TOP-LEVEL-PACKAGES">
        <d:ctr name="TS_T2D36M11I0R0" type="AR-PACKAGE">
            <a:a name="UUID" value="ECUC:14307b58-b65b-4d9b-9409-761abea37352"/>
            <d:lst type="ELEMENTS">
                <d:chc name="Icu" type="AR-ELEMENT" value="MODULE-DEF">
                    <v:ctr type="MODULE-DEF">
                        <a:a name="RELEASE" value="asc:4.0"/>
                        <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                            <ad:ADMIN-DATA>
                                <ad:DOC-REVISIONS>
                                    <ad:DOC-REVISION>
                                        <ad:REVISION-LABEL>4.2.0</ad:REVISION-LABEL>
                                        <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                                        <ad:DATE>2011-11-02</ad:DATE>
                                    </ad:DOC-REVISION>
                                </ad:DOC-REVISIONS>
                            </ad:ADMIN-DATA>
                        </a:a>
                        <a:a name="DESC" 
                             value="EN: Configuration of the Icu (Input Capture Unit) module."/>
                        <a:a name="UUID" value="ECUC:0987da62-0a3f-4281-bb6b-134858e70d06"/>

                        <!-- /** @implements IMPLEMENTATION_CONFIG_VARIANT_Object */ -->
                        <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="LABEL" value="Config Variant"/>
                            <a:a name="UUID" value="ECUC:baafcd2a-eb69-446c-a356-af402b55c606"/>
                            <a:da name="DEFAULT" value="VariantPostBuild"/>
                            <a:da name="RANGE">
                                <a:v>VariantPostBuild</a:v>
                                <a:v>VariantPreCompile</a:v>
                            </a:da>
                        </v:var>
                        
                        <v:lst name="IcuConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                            <a:da name="MIN" value="1"/>
                            <a:da name="INVALID" type="XPath">
                                <a:tst expr="(../IMPLEMENTATION_CONFIG_VARIANT = 'VariantPreCompile') and (num:i(count(../IcuConfigSet/*)) &gt; 1)" true="For VariantPreCompile, there should be only one IcuConfigSet."/>
                            </a:da>

                            <!-- @implements IcuConfigSet_Object -->
                            <v:ctr name="IcuConfigSet" type="MULTIPLE-CONFIGURATION-CONTAINER">
                                <a:a name="DESC">
                                    <a:v><![CDATA[EN: <html>This container is the base for a multiple configuration set
                                        </html>]]>
                                    </a:v>
                                </a:a>
                                <a:a name="UUID" value="ECUC:29f3a3d0-8415-40f8-b4b6-33a5a381d4e7"/>
                            
                                <!-- @implements IcuMaxChannel_Object -->
                                <v:var name="IcuMaxChannel" type="INTEGER">
                                    <a:a name="CALCULATION-FORMULA" value="number of configured icu channels"/>
                                    <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                                    <a:a name="DERIVED" value="TRUE"/>
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN: <html>
                                            <p>The value for the IcuMaxChannel must match with the number of IcuChannel configured 
                                            For calculating the correct value use the CALC button. </p>
                                            <b>Note</b>: Total number of configured channels should be same across all IcuConfigSets. 
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:e2495095-e888-48a5-8be8-0aed388df940"/>
                                    <a:da name="DEFAULT" type="XPath">
                                        <a:tst expr="num:i(num:i(count(node:fallback(../IcuChannel/*,text:split('1 2 3')))))"/>
                                    </a:da>
                                    <a:da name="RANGE" type="XPath">
                                        <a:tst expr="node:fallback(.,1) =num:i(num:i(count(node:fallback(../IcuChannel/*,text:split('1 2 3')))))" false="The value for the IcuMaxChannel must match with the number of IcuChannel configured. For calculating the correct value use the CALC button"/>
                                    </a:da>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;0"/>
                                        <a:tst expr="&lt;=58"/>
                                    </a:da>
                                </v:var>
                                
                                <v:var name="IcuEXT_ISR_InterruptFilterClockPrescaler" type="INTEGER">
                                    <a:a name="LABEL" value="ICU External Interrupt Filter Clock Prescaler"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[<html><p>Configure the clock prescaler which is used to select the clock for all digital filter counters in the SIUL2</p><b>Note</b>This is an Implementation Specific Parameter.</html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:1ce57dfc-f9e6-45ff-a592-6745b777de7d"/>
                                    <a:da name="DEFAULT" value="0"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=15"/>
                                        <a:tst expr="&gt;=0"/>
                                    </a:da>
                                    <a:da name="ENABLE" type="XPath">
                                          <a:tst expr="../IcuChannel/*/Icu_EXT_ISR_IFERDigitalFilter='true'" true="true" false="false"/>
                                    </a:da>
                                </v:var>
                                
                                <v:var name="IcuEXT_ISR_AlternateInterruptFilterClockPrescaler" type="INTEGER">
                                    <a:a name="LABEL" value="ICU External Alternate Interrupt Filter Clock Prescaler"/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[<html><p>Configure the clock alternate prescaler which is used to select the clock for all digital filter counters in the SIUL2. This is only available when Dual Clock mode is activated.</p>
                                            <b>Note</b>This is an Implementation Specific Parameter.</html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:c79524f8-0b4c-431f-848b-488d9a795a91"/>
                                    <a:da name="DEFAULT" value="0"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&lt;=15"/>
                                        <a:tst expr="&gt;=0"/>
                                    </a:da>
                                    <a:da name="ENABLE" type="XPath">
                                          <a:tst expr="((../IcuChannel/*/Icu_EXT_ISR_IFERDigitalFilter='true') and (../../../IcuNonAUTOSAR/IcuEnableDualClockMode ='true'))" true="true" false="false"/>
                                    </a:da>                                
                                </v:var>

                                <v:lst name="IcuChannel" type="MAP">
                                    <a:da name="MIN" value="1"/>
                                    
                                    <!-- @implements IcuChannel_Object -->   
                                    <v:ctr name="IcuChannel" type="IDENTIFIABLE">
                                        <a:a name="DESC" value="EN: Configuration of an individual ICU channel."/>
                                        <a:a name="UUID" value="ECUC:aae83ce5-d107-41fb-ba32-6fa63475d17d"/>
                                    
                                        <!-- @implements IcuChannelId_Object -->
                                        <v:var name="IcuChannelId" type="INTEGER">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN: <html>
                                                        <p>Channel Id of the ICU channel. 
                                                        This value will be assigned to the symbolic name derived of the IcuChannel container short name.</p>
                                                        </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                            <a:a name="UUID" value="ECUC:60839a9e-02ca-41d6-888c-8981c10959c7"/>
                                            <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                            <a:da name="RANGE" type="XPath">
                                                <a:tst expr="(node:fallback(.,1) &gt;= 0) and (node:fallback(.,1) &lt; num:i(count(node:fallback(node:current()/../../*, num:i(50)))) )" false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the Calc button to calculate correct default value."/>
                                                <a:tst expr="text:uniq(node:fallback(../../*/IcuChannelId, text:split('1 2 3')), node:fallback(.,1))" false="Duplicated value, IcuChannelId must be unique across all channels. Use the Calc button to calculate correct default value."/>
                                            </a:da>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&gt;=0"/>
                                                <a:tst expr="&lt;58"/>
                                            </a:da>
                                        </v:var>

                                        <v:var name="IcuHwChannel" type="ENUMERATION">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>Selects the physical HwChannelId Channel.</html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:e3105b1a-9109-4119-864d-69111916afa8"/>
                                                <a:da name="DEFAULT" type="XPath" expr="ecu:list('Icu.IcuConfigSet.IcuChannel.IcuHwChannel')[position()-1=node:fallback(node:current()/../@index, num:i(0))]">
                                            </a:da>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="text:uniq(../../*/IcuHwChannel, .)" false="Duplicate physical channel"/>
                                            </a:da>
                                            <a:da name="RANGE" type="XPath" expr="ecu:list('Icu.IcuConfigSet.IcuChannel.IcuHwChannel') "/>
                                        </v:var>

                                        
                                        <v:var name="Icu_EXT_ISR_IFERDigitalFilter" type="BOOLEAN">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>
                                                    <p>Enable external digital filter counter on the interrupt pads to filter out glitches on the inputs</p>
                                                    <b>Note:</b> This is an Implementation Specific Parameter.
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:7da05670-844c-4a8b-b614-5b758da3890e"/>
                                            <a:da name="DEFAULT" value="false"/>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="(contains(../IcuHwChannel,'IRQ_'))" true="true" false="false"/>
                                            </a:da>
                                            <a:a name="LABEL" value="ICU External Enable Interrupt Filter"/>
                                        </v:var>

                                        <v:var name="Icu_EXT_ISR_IFMCDigitalFilter" type="INTEGER">
                                            <a:a name="LABEL" value="ICU External Interrupt Filter setting"/>
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>
                                                    <p>Maximum Interrupt Filter Counter setting.</p>
                                                    <b>Note:</b> This is an Implementation Specific Parameter.
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="(contains(../IcuHwChannel,'IRQ_')) and (../Icu_EXT_ISR_IFERDigitalFilter = 'true')" true="true" false="false"/>
                                            </a:da>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:85329caa-7938-4afa-8f02-af9dbfc007e7"/>
                                            <a:da name="DEFAULT" value="0"/>
                                            <a:da name="INVALID" type="Range">
                                                <a:tst expr="&lt;=15"/>
                                                <a:tst expr="&gt;=0"/>
                                            </a:da>
                                        </v:var>

                                        <v:var name="IcuDMAChannelEnable" type="BOOLEAN">
                                           <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>
                                                    <b>IcuDMAChannelEnable</b> indicates if the corresponding channel will use DMA for measurement
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:9a9552b0-9353-40aa-9321-d756f6a8e3c4"/>
                                            <a:da name="DEFAULT" value="false"/>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="(contains(../IcuHwChannel,'GTM')) and (../IcuMeasurementMode= 'ICU_MODE_TIMESTAMP')" true="true" false="false"/>
                                            </a:da>
                                        </v:var>

                                        <v:ref name="IcuDMAChannelRef" type="CHOICE-REFERENCE">
                                            <a:a name="LABEL" value="IcuDMAChannelReference"/>
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN:
                                                    <html>
                                                    <b>Icu DMA Channel Reference</b><p>
                                                    Reference to the DMA Channel configure for the Request</p>
                                                    </html>
                                                    ]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                                 type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="(../IcuDMAChannelEnable = 'true')" true="true" false="false"/>
                                            </a:da>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="text:uniq(../../*/IcuDMAChannelRef, .)" false="Duplicate physical channel"/>
                                            </a:da>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="UUID" value="ECUC:b4f7e93a-357d-4f68-acce-0a81d5085b32"/>
                                            <a:da name="REF">
                                                <a:v>ASPathDataOfSchema:/TS_T2D36M11I0R0/Mcl/MclConfigSet/DMAChannel</a:v>
                                            </a:da>
                                        </v:ref>
                                        
                                        
                                        
                                <v:var name="Icu_TIM_ChannelFilter" type="BOOLEAN">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN: <html>
                                            Enables TIM Filter Function (FLT).The TIM submodule provides a configurable filter mechanism for each input signal. <br><br><br>
                                            <h1>Note:</h1> This is an Implementation Specific Parameter.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:780e4fdf-ad5e-428d-9889-8379394446e0"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(contains(../IcuHwChannel,'GTM_TIM'))" true="true" false="false"/>
                                    </a:da>
                                    <a:a name="LABEL" value="TIM Filter Function"/>
                                </v:var>

                                <v:var name="Icu_SignalMeasuringUnitClock" type="ENUMERATION">
                                    <a:a name="LABEL" value="CMU clock source select for channel."/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN: <html>CMU clock source select for channel , used by Signal measuring unit</html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:5a86af5c-77df-417a-b384-2e475a71a98e"/>
                                    <a:da name="DEFAULT" value="CMU_CLK0"/>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(contains(../IcuHwChannel,'GTM_TIM'))" true="true" false="false"/>
                                    </a:da>
                                    <a:da name="RANGE">
                                        <a:v>CMU_CLK0</a:v>
                                        <a:v>CMU_CLK1</a:v>
                                        <a:v>CMU_CLK2</a:v>
                                        <a:v>CMU_CLK3</a:v>
                                        <a:v>CMU_CLK4</a:v>
                                        <a:v>CMU_CLK5</a:v>
                                        <a:v>CMU_CLK6</a:v>
                                        <a:v>CMU_CLK7</a:v>
                                    </a:da>
                                </v:var>
                                <v:var name="Icu_SignalMeasuringUnitClock_Alternative" type="ENUMERATION">
                                            <a:a name="LABEL" value="Icu SignalMeasuringUnit Clock Alternative"/>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>If an GTM channel is being used, this configures the alternate prescaler value for the specific channel.</html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="(contains(../IcuHwChannel,'GTM'))" true="true" false="false"/>
                                            </a:da>
                                            <a:da name="EDITABLE" type="XPath">
                                                <a:tst expr="../../../../../IcuNonAUTOSAR/IcuEnableDualClockMode ='true'"/>
                                            </a:da>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:c8c11e8b-13c0-43fe-be6f-73332b1cce72"/>
                                            <a:da name="DEFAULT" value="CMU_CLK0"/>
                                            <a:da name="RANGE">
                                                <a:v>CMU_CLK0</a:v>
                                                <a:v>CMU_CLK1</a:v>
                                                <a:v>CMU_CLK2</a:v>
                                                <a:v>CMU_CLK3</a:v>
                                                <a:v>CMU_CLK4</a:v>
                                                <a:v>CMU_CLK5</a:v>
                                                <a:v>CMU_CLK6</a:v>
                                                <a:v>CMU_CLK7</a:v>
                                            </a:da>
                                </v:var>
                                
                                <v:var name="Icu_TimeBase" type="ENUMERATION">
                                    <a:a name="LABEL" value="TBU Channel used for ICU_TIMESTAMP."/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN: <html>TBU channel select for channel , used by ICU_TIMESTAMP mode</html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(contains(../IcuMeasurementMode,'ICU_MODE_TIMESTAMP'))" true="true" false="false"/>
                                    </a:da>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:ec1ad04a-68fa-4f5f-bc49-20045547a7e6"/>
                                    <a:da name="DEFAULT" value="TBU_TS0"/>
                                    <a:da name="RANGE">
                                        <a:v>TBU_TS0</a:v>
                                   <!-- <a:v>TBU_TS1</a:v> -->
                                   <!-- <a:v>TBU_TS2</a:v> -->
                                    </a:da>
                                </v:var>
                                
                                <v:var name="Icu_TimeBaseCh0Bits" type="ENUMERATION">
                                    <a:a name="LABEL" value="TBU Channel 0 bits used for ICU_TIMESTAMP."/>
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN: <html>TBU_TS0z bits input select for TIM_CH[n]_GPRz (z: 0, 1).
                                                                    0 Use TBU_TS0z(23..0) to store in TIM0_CH[x]_GPRz.
                                                                    1 Use TBU_TS0z(26..3) to store in TIM0_CH[x]_GPRz.
                                              <h1>Note:</h1>NOTE: This bit is only applicable for TIM0.<br>
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="ENABLE" type="XPath">
                                        <a:tst expr="(contains(../IcuHwChannel,'GTM_TIM') and contains(../IcuMeasurementMode,'ICU_MODE_TIMESTAMP')and contains(../Icu_TimeBase,'TBU_TS0'))" true="true" false="false"/>
                                    </a:da>
                                    <a:a name="ORIGIN" value="Freescale"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:c376807d-f121-4e2d-9603-4ec10b634333"/>
                                    <a:da name="DEFAULT" value="TBU_TS0_23_0"/>
                                    <a:da name="RANGE">
                                        <a:v>TBU_TS0_23_0</a:v>
                                   <!-- <a:v>TBU_TS0_26_3</a:v> -->
                                    </a:da>
                                </v:var>
                                        <!-- @implements IcuDefaultStartEdge_Object -->
                                        <v:var name="IcuDefaultStartEdge" type="ENUMERATION">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN: <html>
                                                    <p>Configures the default-activation-edge which shall be used for this channel</p>
                                                    <p>if there was no activation-edge configured by the call of service Icu_SetActivationCondition().</p>
                                                    <p>In case the Measurement Mode is &quot;IcuSignalMeasurement&quot; and the properties &quot;DutyCycle&quot; or &quot;Period&quot; are set, the edge configured here is used as Default Period Start Edge.</p>
                                                    <p>Implementation Type: Icu_ActivationType</p>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:2cf45adf-8738-4e68-91fb-10b91ed17a39"/>
                                            <a:da name="DEFAULT" value="ICU_RISING_EDGE"/>
                                            <a:da name="RANGE">
                                                <a:v>ICU_BOTH_EDGES</a:v>
                                                <a:v>ICU_FALLING_EDGE</a:v>
                                                <a:v>ICU_RISING_EDGE</a:v>
                                            </a:da>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="((../IcuMeasurementMode = 'ICU_MODE_SIGNAL_MEASUREMENT') and (../IcuSignalMeasurement/IcuSignalMeasurementProperty='ICU_HIGH_TIME') and (.!='ICU_RISING_EDGE') or 
                                                              (../IcuMeasurementMode = 'ICU_MODE_SIGNAL_MEASUREMENT') and (../IcuSignalMeasurement/IcuSignalMeasurementProperty='ICU_LOW_TIME') and (.!='ICU_FALLING_EDGE'))" 
                                                              true = " Error in configuring the default start edge - for measuring the high time should be ICU_RISING_EDGE and for measuring low time should be ICU_FALLING_EDGE"/>
                                             </a:da>
                                        </v:var>

                                        <!-- @implements IcuMeasurementMode_Object -->
                                        <v:var name="IcuMeasurementMode" type="ENUMERATION">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN: <html>
                                                        <p>Configures the measurement mode of this channel.</p>
                                                        <p>User should enable optional parameters with respect to the selected IcuMeasurementMode.</p>
                                                        <p>Implementation Type: Icu_MeasurementModeType</p>
                                                        </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:94546edf-dfa7-47c8-b4cc-46073a23e6c9"/>
                                            <a:da name="DEFAULT" value="ICU_MODE_SIGNAL_EDGE_DETECT"/>
                                            <a:da name="RANGE">
                                                <a:v>ICU_MODE_EDGE_COUNTER</a:v>
                                                <a:v>ICU_MODE_SIGNAL_EDGE_DETECT</a:v>
                                                <a:v>ICU_MODE_SIGNAL_MEASUREMENT</a:v>
                                                <a:v>ICU_MODE_TIMESTAMP</a:v>
                                            </a:da>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="(contains(ecu:list(concat('Icu.ChannelType.',../IcuHwChannel)),node:value(.)))" false="The selected mode is not supported by the Hardware Channel."/>
                                                <a:tst expr="((.='ICU_MODE_EDGE_COUNTER') and (//IcuEdgeCountApi='false' ))" true="If IcuMeasurementMode = 'ICU_MODE_EDGE_COUNTER', IcuEdgeCountApi switch should be set to ON"/>
                                                <a:tst expr="((.='ICU_MODE_SIGNAL_EDGE_DETECT') and (//IcuEdgeDetectApi ='false'))" true="IcuMeasurementMode= 'ICU_MODE_SIGNAL_EDGE_DETECT', IcuEdgeDetectApi switch should be set to ON"/>
                                                <a:tst expr="((.='ICU_MODE_SIGNAL_MEASUREMENT') and ( (//IcuGetDutyCycleValuesApi='false') and (//IcuGetTimeElapsedApi='false') ))" true="IcuMeasurementMode= 'ICU_MODE_SIGNAL_MEASUREMENT', IcuGetTimeElapsedApi or IcuGetDutyCycleValuesApi, switch should be set to ON"/>
                                                <a:tst expr="((.='ICU_MODE_TIMESTAMP') and (//IcuTimestampApi='false'))" true="IcuMeasurementMode= 'ICU_MODE_TIMESTAMP', IcuTimestampApi switch should be set to ON"/>
                                                <a:tst expr="((.='ICU_MODE_SIGNAL_EDGE_DETECT') and (not(node:exists(../IcuSignalEdgeDetection))))" true="IcuMeasurementMode= 'ICU_MODE_SIGNAL_EDGE_DETECT', Enable the IcuSignalEdgeDetection optional container"/> 
                                                <a:tst expr="((.='ICU_MODE_TIMESTAMP') and (not(node:exists(../IcuTimestampMeasurement))))" true="IcuMeasurementMode= 'ICU_MODE_TIMESTAMP', Enable the IcuTimestampMeasurement optional container"/>      
                                                <a:tst expr="((.='ICU_MODE_SIGNAL_MEASUREMENT') and (not(node:exists(../IcuSignalMeasurement))))" true="IcuMeasurementMode= 'ICU_MODE_SIGNAL_MEASUREMENT', Enable the IcuSignalMeasurement optional container"/>
                                            </a:da>
                                        </v:var>

                                        <v:var name="IcuOverflowNotification" type="FUNCTION-NAME">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>
                                                    <p><b>Icu Overflow Notification Handler</b></p>
                                                    <p>In order to activate this field you have to:</p>
                                                    <ul>
                                                        <li>enable <i>IcuOverflowNotificationApi</i>,</li>
                                                        <li>choose one of the modes:
                                                         <ul>
                                                            <li>ICU_MODE_EDGE_COUNTER,</li>
                                                            <li>ICU_MODE_SIGNAL_MEASUREMENT,</li>
                                                           </ul>
                                                        </li>
                                                        <li> to enable overflow detection on the internal counter</li>
                                                    </ul>
                                                    <p><b>Note:</b>
                                                    Due to hardware implementattion, the <i>Icu Overflow Notification</i> 
                                                    is not syncronous with the event for <i>ICU_MODE_SIGNAL_MEASUREMENT</i> 
                                                    modes.</p>
                                                    <p>The notification will be triggered when measurement completes (for ICU_MODE_SIGNAL_MEASUREMENT).</p>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:b1f403da-8f5d-47b0-9bbf-146a2402e69b"/>
                                            <a:da name="DEFAULT" value="NULL_PTR"/>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="((.='NULL') or (.='Null') or (.='false') or (.='FALSE') or (.='null') or (.='null_ptr') or (.='Null_Ptr'))" true="Invalid name of the IcuOverflowNotification. Must be valid C function name, NULL_PTR, &quot;NULL_PTR&quot;."/>
                                                <a:tst expr="normalize-space(.) = '&quot;NULL_PTR&quot;' or normalize-space(.) = 'NULL_PTR' or normalize-space(.) = '&quot;NULL&quot;' or normalize-space(.) = 'NULL' or text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" false="Invalid name of the IcuOverflowNotification. Must be valid C function name, NULL_PTR, &quot;NULL_PTR&quot;, NULL, or &quot;NULL&quot;."/>
                                            </a:da>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="((../../../../../IcuNonAUTOSAR/IcuOverflowNotificationApi='true') and (contains(../IcuMeasurementMode, 'ICU_MODE_EDGE_COUNTER') or contains(../IcuMeasurementMode, 'ICU_MODE_SIGNAL_MEASUREMENT')))" true="true" false="false"/>
                                            </a:da>
                                        </v:var>
                                        

                                        <v:var name="IcuLockableChannel" type="BOOLEAN">
                                           <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>
                                                    <b>IcuLockableChannel</b> indicates if the corresponding channel shall be the object of the configuration locking mechanism.
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="Freescale"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:4ac94fac-6fa9-4af9-b021-1ef017e21b52"/>
                                            <a:da name="DEFAULT" value="false"/>
                                            <a:da name="ENABLE" type="XPath">
                                                <a:tst expr="(../../../../../IcuNonAUTOSAR/IcuRegisterLockingMode='SUPERVISOR_MODE_SOFT_LOCKING')" true="true" false="false"/>
                                            </a:da>
                                        </v:var>


                                        <!-- @implements IcuWakeupCapability_Object -->
                                        <v:var name="IcuWakeupCapability" type="BOOLEAN">
                                            <a:a name="DESC">
                                                <a:v><![CDATA[EN: <html>
                                                    <p>Information about the wakeup-capability of this channel.</p>
                                                    <ul>
                                                        <li>true: Channel is wakeup capable.</li>
                                                        <li>false: Channel is not wakeup capable.</li>
                                                    </ul>
                                                    </html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                            </a:a>
                                            <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                            <a:a name="UUID" value="ECUC:1beaa4ba-15d4-4d8b-bbd1-f82fa7a4e7dd"/>
                                            <a:da name="DEFAULT" value="false"/>
                                            <a:da name="INVALID" type="XPath">
                                                <a:tst expr="((.='true') and num:i(num:i(count(../IcuWakeup/*)))= '0') " true="There is a Icu Wakeup Channel that is missing some configuration. Enable IcuWakeup container and select reference path ."/>
                                                <a:tst expr="((.='true') and (//IcuEnableWakeupApi='false'))" true="If IcuWakeupCapability== ON, IcuEnableWakeupApi and IcuDisableWakeupApi switch should be set to ON"/>
                                                <a:tst expr="((.='true') and ((../IcuMeasurementMode= 'ICU_MODE_EDGE_COUNTER')or (../IcuMeasurementMode= 'ICU_MODE_SIGNAL_MEASUREMENT')or(../IcuMeasurementMode= 'ICU_MODE_TIMESTAMP'))) " true="If IcuHwChannel is configured as Wakeup capable, IcuMeasurementMode should be configured as 'IcuSignalEdgeDetection'."/>
                                            </a:da>
                                        </v:var>
     
                                <v:ctr name="Icu_TIM_FilterParameters" type="IDENTIFIABLE">
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN: <html>This container contains the configuration (parameters) for the TIM Filter Function </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="UUID" value="ECUC:16948eca-76e4-44bf-bbbb-f3e0674ac54a"/>
                                    <a:da name="ENABLE" type="XPath" expr="(../Icu_TIM_ChannelFilter = 'true')"/>
                                   
                                    <v:var name="IcuFilterModeFallingEdge" type="ENUMERATION">
                                        <a:a name="LABEL" value="ICU Filter mode for falling edge."/>
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>The filter modes can be applied individually to the falling and rising edges of an input signal
                                                <h1>Note:</h1> TAKE CARE IF THE FILTER ON ONE EDGE IS CONFIGURED AS IMMEDIATE_EDGE_PROPAGATION AND OTHER AS INDIVIDUAL_DEGLITCH<br>
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:156a2b8d-b810-4562-8367-6fa90384207d"/>
                                        <a:da name="DEFAULT" value="IMMEDIATE_EDGE_PROPAGATION"/>
                                        <a:da name="RANGE">
                                            <a:v>IMMEDIATE_EDGE_PROPAGATION</a:v>
                                            <a:v>INDIVIDUAL_DEGLITCH</a:v>
                                        </a:da>
                                    </v:var>

                                    <v:var name="IcuFilterCounterModeFallingEdge" type="ENUMERATION">
                                        <a:a name="LABEL" value="Filter counter mode for falling edge."/>
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>For Individual de-glitch mode there are two modes for the filter counter: Up/Down Counter and Hold Counter
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:da name="ENABLE" type="XPath">
                                            <a:tst expr="(contains(../IcuFilterModeFallingEdge,'INDIVIDUAL_DEGLITCH'))" true="true" false="false"/>
                                        </a:da>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:0342865d-aaa6-48aa-9076-bbaeeb02800c"/>
                                        <a:da name="DEFAULT" value="UP_DOWN_COUNTER"/>
                                        <a:da name="RANGE">
                                            <a:v>UP_DOWN_COUNTER</a:v>
                                            <a:v>HOLD_COUNTER</a:v>
                                        </a:da>
                                    </v:var>

                                    <v:var name="IcuFilterModeRisingEdge" type="ENUMERATION">
                                        <a:a name="LABEL" value="ICU Filter mode for rising edge."/>
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>The filter modes can be applied individually to the falling and rising edges of an input signal
                                                <h1>Note:</h1> TAKE CARE IF THE FILTER ON ONE EDGE IS CONFIGURED AS IMMEDIATE_EDGE_PROPAGATION AND OTHER AS INDIVIDUAL_DEGLITCH<br>
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:17af98b5-cbdf-446b-8a05-9d066da53d38"/>
                                        <a:da name="DEFAULT" value="IMMEDIATE_EDGE_PROPAGATION"/>
                                        <a:da name="RANGE">
                                            <a:v>IMMEDIATE_EDGE_PROPAGATION</a:v>
                                            <a:v>INDIVIDUAL_DEGLITCH</a:v>
                                        </a:da>
                                    </v:var>

                                    <v:var name="IcuFilterCounterModeRisingEdge" type="ENUMERATION">
                                        <a:a name="LABEL" value="ICU Filter counter mode for rising edge."/>
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>For Individual de-glitch mode there are two modes for the filter counter: Up/Down Counter and Hold Counter
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:da name="ENABLE" type="XPath">
                                            <a:tst expr="(contains(../IcuFilterModeRisingEdge,'INDIVIDUAL_DEGLITCH'))" true="true" false="false"/>
                                        </a:da>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:8f2143b8-5da3-4b55-90af-b5f2a5ec21a6"/>
                                        <a:da name="DEFAULT" value="UP_DOWN_COUNTER"/>
                                        <a:da name="RANGE">
                                            <a:v>UP_DOWN_COUNTER</a:v>
                                            <a:v>HOLD_COUNTER</a:v>
                                        </a:da>
                                    </v:var>
                                    
                                    <v:var name="IcuFilterCounterClock" type="ENUMERATION">
                                        <a:a name="LABEL" value="Icu Filter Counter Clock."/>
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>
                                                If Channel Filter is enabled, this used for select Filter counter frequency.
                                                00 = FLT_CNT counts with CMU_CLK0
                                                01 = FLT_CNT counts with CMU_CLK1
                                                10 = FLT_CNT counts with CMU_CLK6
                                                11 = FLT_CNT counts with CMU_CLK7
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:7004f9fa-017b-4815-82e0-453b5bfec625"/>
                                        <a:da name="DEFAULT" value="FLT_CMU_CLK0"/>
                                        <a:da name="RANGE">
                                            <a:v>FLT_CMU_CLK0</a:v>
                                            <a:v>FLT_CMU_CLK1</a:v>
                                            <a:v>FLT_CMU_CLK6</a:v>
                                            <a:v>FLT_CMU_CLK7</a:v>
                                        </a:da>
                                    </v:var>

                                    <v:var name="IcuFilterParameterFallingEdge" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v><![CDATA[EN: <html>
                                                    This register has different meanings in the various filter modes:
                                                    Immediate edge propagation mode = acceptance time for falling edge.
                                                    Individual deglitch time mode = deglitch time for falling edge.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:48007f8e-6726-481c-b851-6b3326305b45"/>
                                        <a:da name="DEFAULT" value ="0"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&gt;=0"/>
                                            <a:tst expr="&lt;=16777215"/>
                                        </a:da>
                                    </v:var>

                                    <v:var name="IcuFilterParameterRisingEdge" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v><![CDATA[EN: <html>
                                                    This register has different meanings in the various filter modes:
                                                    Immediate edge propagation mode = acceptance time for rising edge.
                                                    Individual deglitch time mode = deglitch time for rising edge.
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="Freescale"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:5262288a-525d-42a8-9b23-ffea9befe8ad"/>
                                        <a:da name="DEFAULT" value ="0"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&gt;=0"/>
                                            <a:tst expr="&lt;=16777215"/>
                                        </a:da>
                                    </v:var>
                                </v:ctr>

                                        <!-- @implements IcuSignalEdgeDetection_Object -->
                                        <v:ctr name="IcuSignalEdgeDetection" type="IDENTIFIABLE">
                                            <a:a name="DESC">
                                                <a:v>
                                                    <![CDATA[EN: <html>This container contains the configuration (parameters) in case the measurement mode is &quot;IcuSignalEdgeDetection&quot;</html>]]>
                                                </a:v>
                                            </a:a>
                                            <a:a name="UUID" value="ECUC:098287a9-6737-4858-84e2-842e133f7b12"/>
                                            <a:a name="OPTIONAL" value="true"/>
                                            <a:da name="ENABLE" value="false"/>
                                            
                                            <!-- @implements IcuSignalNotification_Object -->
                                            <v:var name="IcuSignalNotification" type="FUNCTION-NAME">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN: <html> Notification function for signal notification.</html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:94f7bdd6-7ae4-4383-b4dd-c4d64b56ceba"/>
                                                <a:da name="DEFAULT" value="NULL_PTR"/>
                                                <a:da name="INVALID" type="XPath">
