                                                <a:da name="RANGE">
                                                    <a:v>ICU_CIRCULAR_BUFFER</a:v>
                                                    <a:v>ICU_LINEAR_BUFFER</a:v>
                                                </a:da>
                                            </v:var>

                                            <!-- @implements IcuTimestampNotification_Object -->
                                            <v:var name="IcuTimestampNotification" type="FUNCTION-NAME">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN: <html>Notification function if the number of requested timestamps(Notification interval &gt; 0) are acquired.</html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
