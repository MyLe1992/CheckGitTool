                                                    <a:v>ICU_HIGH_TIME</a:v>
                                                    <a:v>ICU_LOW_TIME</a:v>
                                                    <a:v>ICU_PERIOD_TIME</a:v>
                                                    <a:v>ICU_ACTIVE_TIME</a:v>
                                                </a:da>
                                                <a:da name="INVALID" type="XPath">
                                                    <a:tst expr="((../IcuMeasurementMode= 'ICU_MODE_SIGNAL_MEASUREMENT' ) and (node:exists(IcuSignalMeasurement)))" true="There is a IcuSignalMeasurement Channel that is missing some configuration"/>
                                                    <a:tst expr="((.='ICU_DUTY_CYCLE') and (//IcuGetDutyCycleValuesApi ='false'))" true="IcuSignalMeasurementProperty= 'ICU_DUTY_CYCLE', IcuGetDutyCycleValuesApi switch should be set to ON"/>
                                                    <a:tst expr="((.='ICU_HIGH_TIME') and (//IcuGetTimeElapsedApi ='false'))" true="IcuSignalMeasurementProperty = 'ICU_HIGH_TIME', IcuGetTimeElapsedApi switch should be set to ON"/>
                                                    <a:tst expr="((.='ICU_LOW_TIME') and (//IcuGetTimeElapsedApi ='false'))" true="IcuSignalMeasurementProperty = 'ICU_LOW_TIME', IcuGetTimeElapsedApi switch should be set to ON"/>
                                                    <a:tst expr="((.='ICU_ACTIVE_TIME') and (//IcuGetTimeElapsedApi ='false'))" true="IcuSignalMeasurementProperty = 'ICU_ACTIVE_TIME', IcuGetTimeElapsedApi switch should be set to ON"/>
                                                    <a:tst expr="((.='ICU_PERIOD_TIME') and (//IcuGetTimeElapsedApi ='false'))" true="IcuSignalMeasurementProperty = 'ICU_PERIOD_TIME', IcuGetTimeElapsedApi switch should be set to ON"/>
                                                </a:da>
                                            </v:var>
                                        </v:ctr>

                                        <!-- @implements IcuTimestampMeasurement_Object -->
                                        <v:ctr name="IcuTimestampMeasurement" type="IDENTIFIABLE">
                                            <a:a name="DESC" value="EN: This container contains the configuration (parameters) in case the measurement mode is &quot;IcuTimestamp&quot;"/>
