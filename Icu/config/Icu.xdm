                                                </a:a>
                                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="UUID" value="ECUC:c1d36a80-82f3-4b9c-81ff-d470ffb9a002"/>
                                                <a:da name="DEFAULT" value="NULL_PTR"/>
                                                <a:da name="INVALID" type="XPath">
                                                    <a:tst expr="((.='NULL') or (.='Null') or (.='false') or (.='FALSE') or (.='null') or (.='null_ptr') or (.='Null_Ptr'))" true="Invalid name of the IcuTimestampNotification. Must be valid C function name, NULL_PTR, &quot;NULL_PTR&quot;."/>
                                                    <a:tst expr="normalize-space(.) = '&quot;NULL_PTR&quot;' or normalize-space(.) = 'NULL_PTR' or normalize-space(.) = '&quot;NULL&quot;' or normalize-space(.) = 'NULL' or text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" false="Invalid name of the IcuTimestampNotification. Must be valid C function name, NULL_PTR, &quot;NULL_PTR&quot;, NULL, or &quot;NULL&quot;."/>
                                                </a:da>
                                            </v:var>
                                        </v:ctr>

                                        <!-- @implements IcuWakeup_Object -->
                                        <v:ctr name="IcuWakeup" type="IDENTIFIABLE">
